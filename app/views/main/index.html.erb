<div>
  placeholder
</div>
<% content_for :javascript do %>
  <script>var access_token = "<%= @access_token %>";</script>
  <%= javascript_include_tag "/js/ActivityLoader.js" %>
<% end %>
<div class="row">
  <div class="column">
    <div class="tableName">
      Activities
    </div>
    <div class="tableFixHead">
      <table id="activityList">
        <thead>
          <tr>
            <th style="width: 30%">Name</th>
            <th style="width: 30%">Date</th>
            <th style="width: 8%">Type</th>
            <th style="width: 8%"><span class="unit"></span></th>
            <th style="width: 12%">t</th>
            <th style="width: 12%">t/<span class="unit"></span></th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
  <div class="column">
    <div class="tableName">
      Pace calculation activities
    </div>
    <div class="tableFixHead">
      <table id="calculateList">
        <thead>
          <tr>
            <th style="width: 30%">Name</th>
            <th style="width: 30%">Date</th>
            <th style="width: 8%">Type</th>
            <th style="width: 8%"><span class="unit"></span></th>
            <th style="width: 12%">t</th>
            <th style="width: 12%">t/<span class="unit"></span></th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>